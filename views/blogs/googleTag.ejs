<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../header') %>
    <title>Google Tag</title> 
</head>

<body class="landing-page sidebar-collapse" style="padding-top: 60px;">
    <%- include('../navbar') %>
    <div class="wrapper">
        <div class="section rounded">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 mr-auto">
                        <p>Google Tag</p>
                    </div>
                </div>
                <div class="card">
                    <div class="row card-body">
                        <div class="col-6 py-2">
                            <p>Google Manager Scirpt</p>
                            <% results.forEach(function(res){ %>
                            <p class="form-control"><%= res.google_manager %></p>
                            <% }) %> 
                        </div>
                        <div class="col-6 py-2">
                            <p>Google Analytic Scirpt</p>
                            <% results.forEach(function(res){ %>
                            <p class="form-control"><%= res.google_analytic %></p>
                            <% }) %> 
                        </div>
                        <div class="col-6 py-2 form-group">
                            <p>Google Verification Scirpt</p>
                            <% results.forEach(function(res){ %>
                            <p class="form-control"><%= res.google_verification %></p>
                            <% }) %> 
                        </div>
                    </div>
                    <div class="col-12 sumbit text-center">
                        <a href="/admin/add-googletag"><button class="btn btn-primary" style="width:60%;">แก้ไข</button></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
<%- include('../script') %>
<script type="text/javascript">
    $(document).ready(()=>{
          var $nav = $('.fixed-top');
              $nav.removeClass('navbar-transparent')
      });
  </script>